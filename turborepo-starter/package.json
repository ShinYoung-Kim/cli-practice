    "test-ct": "playwright test -c playwright-ct.config.ts"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.2",
    "@playwright/experimental-ct-react": "^1.44.0",
    "@playwright/test": "^1.44.0",
    "@types/node": "18.19.33",
    "prettier": "^3.2.5",
    "turbo": "latest"
  },
  "packageManager": "pnpm@8.9.0",
  "dependencies": {
    "@turbo/gen": "^1.13.3"
  }
}
